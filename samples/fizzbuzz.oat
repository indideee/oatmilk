@import("io");

fn main() {

  // range returns an array of ints between 2 numbers e.g. range(0,5) -> [0, 1, 2, 3, 4, 5]
  for i in range(1, 20) {
    if i % 3 == 0 && i % 5 == 0 {
      io:println("FizzBuzz");
    } else if i % 3 == 0 {
      io:println("Fizz");
    } else if i % 5 == 0 {
      io:println("Buzz");
    } else {
      io:println(i);
    }
  }
}



